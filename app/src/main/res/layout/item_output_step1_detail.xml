<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/output_model_Detail"
    android:visibility="gone">

    <TextView
        android:id="@+id/output_rankTextView"
        android:layout_width="match_parent"
        android:text="rank"
        android:textColor="@color/grey_150"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/output_relevanceTextView"
        android:layout_width="match_parent"
        android:text="relevance"
        android:textColor="@color/grey_150"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/output_hotnessTextView"
        android:layout_width="match_parent"
        android:text="hotness"
        android:textColor="@color/grey_150"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/output_negativeIssuesTextView"
        android:layout_width="match_parent"
        android:text="issue"
        android:textColor="@color/grey_150"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/output_imageTextView"
        android:layout_width="match_parent"
        android:text="imageText"
        android:textColor="@color/grey_150"
        android:layout_height="wrap_content"/>
</LinearLayout>
